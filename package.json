{
  "name": "takeoff-app",
  "version": "1.0.0",
  "description": "private cabinet react node",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node app.js",
    "server": "nodemon app.js",
    "client": "npm run start --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run client\" \"npm run server\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cricket-creator/takeoff-app.git"
  },
  "keywords": [
    "private",
    "cabinet",
    "react",
    "node"
  ],
  "author": "Pavel Rakhaev <rakhaevpavel@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/cricket-creator/takeoff-app/issues"
  },
  "homepage": "https://github.com/cricket-creator/takeoff-app#readme",
  "dependencies": {
    "bcrypt": "^5.0.1",
    "config": "^3.3.7",
    "express": "^4.17.2",
    "express-validator": "^6.14.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.8"
  },
  "devDependencies": {
    "concurrently": "^7.0.0",
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.15"
  }
}
